{
  "name": "coffee-order-system",
  "version": "0.1.0",
  "description": "Coffee ordering system with React frontend and Node.js backend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "install-server": "cd backend && npm install",
    "install-client": "cd frontend && npm install",
    "install-all": "npm run install-server && npm run install-client",
    "build": "cd frontend && npm run build",
    "lint": "eslint . --ext .js,.jsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx --ignore-path .gitignore --fix",
    "format": "prettier --write . --ignore-path .gitignore",
    "test": "echo \"Tests will be added later\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yunona46/coffee-order-system.git"
  },
  "keywords": ["coffee", "order-system", "react", "nodejs", "express"],
  "author": "Yunona",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "eslint": "^8.0.0",
    "prettier": "^2.8.0"
  }
}
