{
  \"name\": \"coffee-order-system\",
  \"version\": \"1.0.0\",
  \"description\": \"Coffee ordering system with React frontend and Node.js backend\",
  \"type\": \"commonjs\",
  \"scripts\": {
    \"dev\": \"concurrently \\\"npm run server\\\" \\\"npm run client\\\"\",
    \"server\": \"cd backend && npm run dev\",
    \"client\": \"cd frontend && npm start\",
    \"install-all\": \"npm install && cd backend && npm install && cd ../frontend && npm install\",
    \"build\": \"cd frontend && npm run build\",
    \"start\": \"cd backend && npm start\",
    \"clean\": \"npm run clean:node_modules && npm run clean:build\",
    \"clean:node_modules\": \"rimraf node_modules backend/node_modules frontend/node_modules\",
    \"clean:build\": \"rimraf dist build .next .nuxt\"
  },
  \"repository\": {
    \"type\": \"git\",
    \"url\": \"https://github.com/yunona46/coffee-order-system.git\"
  },
  \"keywords\": [\"coffee\", \"order\", \"react\", \"nodejs\", \"express\"],
  \"author\": \"yunona46\",
  \"license\": \"MIT\",
  \"devDependencies\": {
    \"concurrently\": \"^8.0.0\",
    \"rimraf\": \"^5.0.0\"
  }
}
